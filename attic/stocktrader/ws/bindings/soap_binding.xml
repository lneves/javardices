<?xml version="1.0" encoding="UTF-8"?>
<binding name="binding" trim-whitespace="true">

	<namespace uri="http://schemas.xmlsoap.org/soap/envelope/" prefix="soap" />

	<include path="trade_binding.xml" />

	<mapping name="Envelope" class="trade.xml.SoapEnvelope" ns="http://schemas.xmlsoap.org/soap/envelope/" ordered="false" flexible="true">
		<structure field="header" usage="optional" ns="http://schemas.xmlsoap.org/soap/envelope/" />
		<structure field="body" usage="required" ns="http://schemas.xmlsoap.org/soap/envelope/" />
	</mapping>

	<mapping name="Header" class="trade.xml.SoapHeader" ns="http://schemas.xmlsoap.org/soap/envelope/" />

	<mapping name="Body" class="trade.xml.SoapBody" ns="http://schemas.xmlsoap.org/soap/envelope/" ordered="false" flexible="true">
		<structure field="fault" usage="optional" />

		<structure field="buyRequest" usage="optional" />
		<structure field="buyResponse" usage="optional" />

		<structure field="cancelOrderRequest" usage="optional" />
		<structure field="cancelOrderResponse" usage="optional" />

		<structure field="completeOrderRequest" usage="optional" />
		<structure field="completeOrderResponse" usage="optional" />

		<structure field="createQuoteRequest" usage="optional" />
		<structure field="createQuoteResponse" usage="optional" />

		<structure field="getAccountDataRequest" usage="optional" />
		<structure field="getAccountDataResponse" usage="optional" />

		<structure field="getAccountProfileDataRequest" usage="optional" />
		<structure field="getAccountProfileDataResponse" usage="optional" />

		<structure field="getAllQuotesRequest" usage="optional" />
		<structure field="getAllQuotesResponse" usage="optional" />

		<structure field="getClosedOrdersRequest" usage="optional" />
		<structure field="getClosedOrdersResponse" usage="optional" />

		<structure field="getHoldingRequest" usage="optional" />
		<structure field="getHoldingResponse" usage="optional" />

		<structure field="getHoldingsRequest" usage="optional" />
		<structure field="getHoldingsResponse" usage="optional" />

		<structure field="getMarketSummaryRequest" usage="optional" />
		<structure field="getMarketSummaryResponse" usage="optional" />

		<structure field="getOrdersRequest" usage="optional" />
		<structure field="getOrdersResponse" usage="optional" />

		<structure field="getQuoteRequest" usage="optional" />
		<structure field="getQuoteResponse" usage="optional" />

		<structure field="loginRequest" usage="optional" />
		<structure field="loginResponse" usage="optional" />

		<structure field="logoutRequest" usage="optional" />
		<structure field="logoutResponse" usage="optional" />

		<structure field="orderCompletedRequest" usage="optional" />
		<structure field="orderCompletedResponse" usage="optional" />

		<structure field="queueOrderRequest" usage="optional" />
		<structure field="queueOrderResponse" usage="optional" />

		<structure field="registerRequest" usage="optional" />
		<structure field="registerResponse" usage="optional" />

		<structure field="resetTradeRequest" usage="optional" />
		<structure field="resetTradeResponse" usage="optional" />

		<structure field="sellRequest" usage="optional" />
		<structure field="sellResponse" usage="optional" />

		<structure field="updateAccountProfileRequest" usage="optional" />
		<structure field="updateAccountProfileResponse" usage="optional" />

		<structure field="updateQuotePriceVolumeRequest" usage="optional" />
		<structure field="updateQuotePriceVolumeResponse" usage="optional" />


	</mapping>

	<mapping name="Fault" class="trade.xml.SoapFault" ns="http://schemas.xmlsoap.org/soap/envelope/">
		<value name="faultcode" field="faultCode" usage="optional" />
		<value name="faultstring" field="faultString" usage="optional" />
		<value name="detail" field="detail" usage="optional" />
	</mapping>

</binding>