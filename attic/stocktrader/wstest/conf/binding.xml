<?xml version="1.0" encoding="UTF-8"?>
<binding>

	<namespace uri="http://schemas.xmlsoap.org/soap/envelope/" prefix="soap" />

	<mapping name="Envelope" class="org.caudexorigo.wstest.ws.xml.SoapEnvelope" ns="http://schemas.xmlsoap.org/soap/envelope/">
		<structure field="header" usage="optional" />
		<structure field="body" usage="required" />
	</mapping>

	<mapping name="Header" class="org.caudexorigo.wstest.ws.xml.SoapHeader" ns="http://schemas.xmlsoap.org/soap/envelope/" />

	<mapping name="Body" class="org.caudexorigo.wstest.ws.xml.SoapBody" ns="http://schemas.xmlsoap.org/soap/envelope/">
		<structure field="echoList" usage="optional" />
		<structure field="echoListResponse" usage="optional" />
		<structure field="echoStruct" usage="optional" />
		<structure field="echoStructResponse" usage="optional" />
		<structure field="echoSynthetic" usage="optional" />
		<structure field="echoSyntheticResponse" usage="optional" />
		<structure field="echoVoid" usage="optional" />
		<structure field="echoVoidResponse" usage="optional" />
		<structure field="getOrder" usage="optional" />
		<structure field="getOrderResponse" usage="optional" />
		<structure field="fault" usage="optional" />
	</mapping>

	<mapping name="Fault" class="org.caudexorigo.wstest.ws.xml.SoapFault" ns="http://schemas.xmlsoap.org/soap/envelope/" ordered="false" flexible="true">
		<value name="faultcode" field="faultCode" usage="optional" />
		<value name="faultstring" field="faultString" usage="optional" />
		<value name="detail" field="detail" usage="optional" />
	</mapping>

	<mapping class="org.caudexorigo.wstest.domain.TestStruct" abstract="true">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="i" field="i" usage="required" />
		<value name="f" field="f" usage="required" />
		<value name="s" field="s" usage="required" />
	</mapping>

	<mapping class="org.caudexorigo.wstest.domain.COfTestStruct" abstract="true">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<collection field="tsList" create-type="java.util.ArrayList">
			<structure name="l" type="org.caudexorigo.wstest.domain.TestStruct" />
		</collection>
	</mapping>

	<mapping class="org.caudexorigo.wstest.domain.TestNode" abstract="true">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure map-as="org.caudexorigo.wstest.domain.TestStruct" />
		<structure name="Next" field="next" usage="optional" type="org.caudexorigo.wstest.domain.TestNode" />
	</mapping>

	<mapping name="LineItem" class="org.caudexorigo.wstest.domain.LineItem">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="OrderId" field="orderId" usage="required" />
		<value name="ItemId" field="itemId" usage="required" />
		<value name="ProductId" field="productId" usage="required" />
		<value name="ProductDescription" field="productDescription" usage="required" />
		<value name="OrderQuantity" field="orderQuantity" usage="required" />
		<value name="UnitPrice" field="unitPrice" usage="required" />
	</mapping>

	<mapping name="LineItems" class="org.caudexorigo.wstest.domain.COfLineItem">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<collection field="lineItemList" create-type="java.util.ArrayList">
			<structure map-as="org.caudexorigo.wstest.domain.LineItem" />
		</collection>
	</mapping>

	<mapping class="org.caudexorigo.wstest.domain.Address" abstract="true">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="FirstName" field="firstName" usage="required" />
		<value name="LastName" field="lastName" usage="required" />
		<value name="Address1" field="address1" usage="required" />
		<value name="Address2" field="address2" usage="required" />
		<value name="City" field="city" usage="required" />
		<value name="State" field="state" usage="required" />
		<value name="Zip" field="zip" usage="required" />
	</mapping>

	<mapping name="Customer" class="org.caudexorigo.wstest.domain.Customer">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="CustomerId" field="customerId" usage="required" />
		<value name="ContactFirstName" field="contactFirstName" usage="required" />
		<value name="ContactLastName" field="contactLastName" usage="required" />
		<value name="ContactPhone" field="contactPhone" usage="required" />
		<value name="LastActivityDate" field="lastActivityDate" usage="required" />
		<value name="CreditCardNumber" field="creditCardNumber" usage="required" />
		<value name="CreditCardExpirationDate" field="creditCardExpirationDate" usage="required" />
		<structure name="BillingAddress" field="billingAddress" usage="required" type="org.caudexorigo.wstest.domain.Address" />
		<structure name="ShippingAddress" field="shippingAddress" usage="required" type="org.caudexorigo.wstest.domain.Address" />
	</mapping>

	<mapping name="GetOrderResult" class="org.caudexorigo.wstest.domain.Order">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="OrderId" field="orderId" usage="required" />
		<value name="OrderStatus" field="orderStatus" usage="required" />
		<value name="OrderDate" field="orderDate" usage="required" />
		<value name="OrderTotalAmount" field="orderTotalAmount" usage="required" />
		<structure field="customer" usage="required" map-as="org.caudexorigo.wstest.domain.Customer" />
		<structure field="cOfLineItem" usage="required" map-as="org.caudexorigo.wstest.domain.COfLineItem" />
	</mapping>

	<mapping name="EchoSynthetic" class="org.caudexorigo.wstest.ws.api.EchoSynthetic">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure name="synth" field="synth" usage="optional" type="org.caudexorigo.wstest.domain.Synthetic" />
	</mapping>

	<mapping class="org.caudexorigo.wstest.domain.Synthetic" abstract="true">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="Strng" field="strng" usage="optional" />
		<structure name="Strct" field="strct" usage="optional" type="org.caudexorigo.wstest.domain.TestStruct" />
		<value name="Barray" field="barray" usage="optional" />
	</mapping>

	<mapping name="EchoSyntheticResponse" class="org.caudexorigo.wstest.ws.api.EchoSyntheticResponse">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure name="EchoSyntheticResult" field="echoSyntheticResult" usage="optional" type="org.caudexorigo.wstest.domain.Synthetic" />
	</mapping>

	<mapping name="EchoStruct" class="org.caudexorigo.wstest.ws.api.EchoStruct">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure name="array" field="array" usage="required" type="org.caudexorigo.wstest.domain.COfTestStruct" />
	</mapping>

	<mapping name="EchoStructResponse" class="org.caudexorigo.wstest.ws.api.EchoStructResponse">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure name="EchoStructResult" field="echoStructResult" usage="required" type="org.caudexorigo.wstest.domain.COfTestStruct" />
	</mapping>

	<mapping name="EchoVoid" class="org.caudexorigo.wstest.ws.api.EchoVoid">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
	</mapping>

	<mapping name="EchoVoidResponse" class="org.caudexorigo.wstest.ws.api.EchoVoidResponse">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
	</mapping>

	<mapping name="EchoList" class="org.caudexorigo.wstest.ws.api.EchoList">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure name="list" field="list" usage="optional" type="org.caudexorigo.wstest.domain.TestNode" />
	</mapping>

	<mapping name="EchoListResponse" class="org.caudexorigo.wstest.ws.api.EchoListResponse">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure name="EchoListResult" field="echoListResult" usage="optional" type="org.caudexorigo.wstest.domain.TestNode" />
	</mapping>

	<mapping name="GetOrder" class="org.caudexorigo.wstest.ws.api.GetOrder">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<value name="orderId" field="orderId" usage="required" />
		<value name="customerId" field="customerId" usage="required" />
		<value name="messageSize" field="messageSize" usage="required" />
	</mapping>

	<mapping name="GetOrderResponse" class="org.caudexorigo.wstest.ws.api.GetOrderResponse">
		<namespace uri="uri:WSTestWeb-TestService" default="elements" />
		<structure field="getOrderResult" usage="required" map-as="org.caudexorigo.wstest.domain.Order" />
	</mapping>

</binding>